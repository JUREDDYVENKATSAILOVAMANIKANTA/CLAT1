#include <stdio.h>

int main ()
{
int divident, divisor, quotient, Remainder;

printf ("Enter divident: ");
scanf ("%d", &divident);
printf ("Enter divisor: ");
scanf ("%d", &divisor);

quotient= divident / divisor;
Remainder= divident % divisor;

printf ("Quotient = %d\n", quotient);
printf ("Remainder = %d", Remainder);

return (0);
